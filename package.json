  "keywords": [
    "gatsby",
    "graphql"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --color",
    "lint:format": "npm run lint -- --fix",
    "check:spelling": "cspell 'src/**'",
    "check:types": "tsc",
